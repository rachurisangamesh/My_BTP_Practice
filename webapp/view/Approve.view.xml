<mvc:View xmlns:core="sap.ui.core" controllerName="com.teche.sd.project1.controller.S1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
        <Select items="{y>/loadproData}">
            <items>
            <core:Item text="{y>proname}"></core:Item>
            </items>
        </Select>

            <!-- <Label text="Emp name"/>
            <Input /> -->
            <List noDataText="Select your protein type" headerText="Protein list" mode="MultiSelect" items="{x>/loadData}">
                <items>
                    <ObjectListItem intro="{x>ProCode}" title="{x>ProName}" number="{x>Quantity}" > 
                        <attributes>
                            <ObjectAttribute text="Price = ${x>Price}" ></ObjectAttribute>
                        </attributes>
                    </ObjectListItem>
                    
                </items>
            </List>
            <Button text="Submit" press="handlsubmit" />

        </content>
    </Page>
</mvc:View>
